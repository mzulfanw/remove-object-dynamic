    "rootDir": "./",
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "declaration": true
  },
  "include": [
    "src/**/*.ts",
    // "jest.config.ts"
